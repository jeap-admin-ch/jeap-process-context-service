{
  "title": "JEAP Process Context UI",
  "name": "jeap-process-context-ui",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "organization": {
    "name": "Federal Office of Information Technology, Systems and Telecommunication FOITT",
    "url": "http://www.bit.admin.ch/"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build --base-href /process-context/",
    "build-prod": "ng build -c production --base-href /process-context/",
    "cypress:run": "cypress run --config video=false --component",
    "check-licenses": "apache2-license-checker",
    "generateLicenseFile": "license-checker --customPath custom-license-md-format.json --markdown > ../THIRD-PARTY-FRONTEND-LICENSES.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.7",
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.3.7",
    "@angular/compiler": "^20.3.7",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/material": "^20.2.10",
    "@angular/platform-browser": "^20.3.7",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.7",
    "@ngx-translate/core": "^17.0.0",
    "@oblique/oblique": "^14.0.1",
    "@popperjs/core": "^2.0.0",
    "@quadrel-services/qd-auth": "^19.0.12",
    "angular-oauth2-oidc": "^20.0.2",
    "cypress": "^15.5.0",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-eslint/builder": "20.4.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/schematics": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/build": "^20.3.7",
    "@angular/cli": "~20.3.7",
    "@angular/compiler-cli": "^20.3.7",
    "@bbc/apache2-license-checker": "^1.1.4",
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "js-cookie": "^3.0.5",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.38.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "2.0.0",
    "license-checker": "^25.0.1",
    "ng-mocks": "^14.14.0",
    "prettier": "^3.2.5",
    "typescript": "5.8.3"
  },
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4,
    "sonar56x": true
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run format"
    }
  }
}
